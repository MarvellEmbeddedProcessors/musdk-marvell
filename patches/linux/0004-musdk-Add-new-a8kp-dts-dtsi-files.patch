+/*
+ * Device Tree file for the Armada 7040 SoC, made of an AP806 Quad and
+ * one CP110.
+ */
+
+/ {
+	model = "Marvell 8080 board";
+	compatible = "marvell,armada-8080", "marvell,armada-ap810-octa",
+				"marvell,armada-ap810";
+
+	ap0 {
+		config-space {
+			musdk_uio {
+				compatible = "marvell,musdk-uio";
+				dma-coherent;
+				status = "okay";
+			};
+		};
+	};
+};
