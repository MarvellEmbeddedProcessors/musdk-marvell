+/*
+ * Device Tree file for the Armada 7040 SoC, made of an AP806 Quad and
+ * one CP110.
+ */
+
+ap0 {
+	config-space {
+		musdk_uio {
+			compatible = "marvell,musdk-uio";
+			dma-coherent;
+			status = "okay";
+		};
+		musdk_uio_region0 {
+			compatible = "marvell,musdk-uio";
+			dma-coherent;
+			status = "okay";
+			uio-mem-region = <0>;
+		};
+	};
+};
+ap1 {
+	config-space {
+		musdk_uio_region1 {
+			compatible = "marvell,musdk-uio";
+			dma-coherent;
+/* Temporarily disable CMA on AP1, until DRAM on AP1 is supported
+			memory-region = <&ap1_mem>;
+*/
+			status = "okay";
+			uio-mem-region = <1>;
+		};
+	};
+};
