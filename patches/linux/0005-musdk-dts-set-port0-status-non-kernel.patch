From 7ddb03b60a8c51715836063f7d77789f52de3c46 Mon Sep 17 00:00:00 2001
From: Lior Amsalem <alior@marvell.com>
Date: Mon, 19 Dec 2016 11:29:04 +0200
Subject: [PATCH 5/5] musdk: dts set port0 status non kernel

Signed-off-by: Yuval Caduri <cyuval@marvell.com>
---
 arch/arm64/boot/dts/marvell/armada-7040-db-router.dts | 2 +-
 arch/arm64/boot/dts/marvell/armada-8040-db-A.dts      | 4 ++--
 arch/arm64/boot/dts/marvell/armada-8040-db-router.dts | 4 ++--
 3 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/arm64/boot/dts/marvell/armada-7040-db-router.dts b/arch/arm64/boot/dts/marvell/armada-7040-db-router.dts
index ae82328..75288b3 100644
--- a/arch/arm64/boot/dts/marvell/armada-7040-db-router.dts
+++ b/arch/arm64/boot/dts/marvell/armada-7040-db-router.dts
@@ -143,7 +143,7 @@
 			};
 			ppv22@000000 {
 				eth0: eth0@010000 {
-					status = "okay";
+					status = "non-kernel";
 				};
 				eth1: eth1@020000 {
 					status = "okay";
diff --git a/arch/arm64/boot/dts/marvell/armada-8040-db-A.dts b/arch/arm64/boot/dts/marvell/armada-8040-db-A.dts
index e0106bd..b830258 100644
--- a/arch/arm64/boot/dts/marvell/armada-8040-db-A.dts
+++ b/arch/arm64/boot/dts/marvell/armada-8040-db-A.dts
@@ -118,7 +118,7 @@
 				pinctrl-0 = <&cpm_ge2_rgmii_pins>;
 
 				eth0: eth0@010000 {
-					status = "okay";
+					status = "non-kernel";
 				};
 				eth1: eth1@020000 {
 					status = "disabled";
@@ -176,7 +176,7 @@
 				pinctrl-0 = <&cps_ge1_rgmii_pins>;
 
 				eth0_1: eth0@010000 {
-					status = "okay";
+					status = "non-kernel";
 				};
 				eth1_1: eth1@020000 {
 					status = "okay";
diff --git a/arch/arm64/boot/dts/marvell/armada-8040-db-router.dts b/arch/arm64/boot/dts/marvell/armada-8040-db-router.dts
index c9ca277..01c5b96 100644
--- a/arch/arm64/boot/dts/marvell/armada-8040-db-router.dts
+++ b/arch/arm64/boot/dts/marvell/armada-8040-db-router.dts
@@ -149,7 +149,7 @@
 				pinctrl-0 = <&cpm_ge2_rgmii_pins>;
 
 				eth0: eth0@010000 {
-					status = "okay";
+					status = "non-kernel";
 				};
 				eth1: eth1@020000 {
 					status = "disabled";
@@ -204,7 +204,7 @@
 				pinctrl-0 = <&cps_ge1_rgmii_pins>;
 
 				eth0_1: eth0@010000 {
-					status = "okay";
+					status = "non-kernel";
 				};
 				eth1_1: eth1@020000 {
 					status = "okay";
-- 
